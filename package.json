{
    "name": "@appliberatedhwa/dafprank",
    "version": "1.0.0",
    "description": "Delayed Auditory Feedback prank app",
    "author": "Appliberated",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AppliberatedHWA/dafprank.git"
    },
    "bugs": {
        "url": "https://github.com/AppliberatedHWA/dafprank/issues"
    },
    "homepage": "https://github.com/AppliberatedHWA/dafprank",
    "eslintConfig": {
        "env": { "browser": true, "es6": true },
        "extends": "eslint:all",
        "rules": {
            "dot-location": "off",
            "func-style": "off",
            "linebreak-style": ["error", "windows"],
            "max-len": ["error", 120],
            "max-params": ["error", 5],
            "multiline-ternary": "off",
            "no-console": "off",
            "no-ternary": "off",
            "object-curly-spacing": "off",
            "object-property-newline": "off",
            "one-var": "off",
            "padded-blocks": "off",
            "quote-props": "off",
            "space-before-function-paren": "off"
        }
    }
}